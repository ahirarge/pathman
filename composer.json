{
    "name": "ahir/pathman",
    "type": "library",
    "description": "Laravel Directory Creator",
    "keywords": ["laravel", "directory"],
    "homepage": "https://github.com/ahirarge/pathman",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahir Ar-Ge",
            "email": "development@ahir.com.tr"
        },
        {
            "name": "Özgür Adem Işıklı",
            "email": "i.ozguradem@gmail.com",
            "role": "Creator & Designer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.2.*",
        "orchestra/testbench": "2.1.*"
    },
    "replace": {
        "phpseclib/phpseclib": "0.3.*"
    },
    "autoload": {
        "classmap": [
            "src/config"
        ],
        "psr-0": {
            "Ahir\\Pathman\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ],
    "minimum-stability": "stable"
}
